<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ModifyPartController">
    <children>
        <Label layoutX="40.0" layoutY="44.0" text="Modify Part">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <RadioButton fx:id="inHouseButton" layoutX="150.0" layoutY="45.0" mnemonicParsing="false" onMouseClicked="#setTextMachineID" text="In-House">
            <font>
                <Font size="14.0" />
            </font>
         <toggleGroup>
            <ToggleGroup fx:id="inOrOutTG" />
         </toggleGroup>
        </RadioButton>
        <RadioButton fx:id="outsourcedButton" layoutX="250.0" layoutY="45.0" mnemonicParsing="false" onMouseClicked="#setTextCompanyName" text="Outsourced" toggleGroup="$inOrOutTG">
            <font>
                <Font size="14.0" />
            </font>
        </RadioButton>
        <TextField fx:id="partIDField" disable="true" layoutX="120.0" layoutY="130.0" promptText="Part ID" />
        <Label layoutX="85.0" layoutY="132.0" text="ID">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="partNameField" layoutX="120.0" layoutY="170.0" />
        <TextField fx:id="partInvField" layoutX="120.0" layoutY="210.0" />
        <TextField fx:id="partPriceField" layoutX="120.0" layoutY="250.0" />
        <TextField fx:id="partMaxField" layoutX="120.0" layoutY="290.0" />
        <TextField fx:id="sourceField" layoutX="120.0" layoutY="330.0" />
        <TextField fx:id="partMinField" layoutX="350.0" layoutY="290.0" />
        <Button layoutX="130.0" layoutY="400.0" mnemonicParsing="false" text="Save">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button layoutX="210.0" layoutY="400.0" mnemonicParsing="false" onAction="#toMain" text="Cancel">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Label layoutX="70.0" layoutY="171.0" text="Name">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="80.0" layoutY="212.0" text="Inv">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="45.0" layoutY="252.0" text="Price/Cost">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="70.0" layoutY="292.0" text="Max">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="305.0" layoutY="292.0" text="Min">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="sourceLabel" layoutX="25.0" layoutY="332.0" text="Label" />
    </children>
</AnchorPane>
